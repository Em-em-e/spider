{
  "manifest_version": 2,
  "version": "1.0.1",
  "name": "百度百家登录助手",
  "description": "百度百家登录助手",
  "browser_action": {

  },
  "permissions": [
  	"cookies",
    "*://*.baidu.com/*",
  	"browsingData",                                            
    "tabs",                                               
    "notifications",
    "https://mp.toutiao.com/profile_v2/income-overview/*",
    "http://localhost:8080/*",
    "http://123.206.231.254/spider/*"
  ],
  "background": {                      
                     
  },
  "content_scripts": [
    {
      "matches": ["http://baijiahao.baidu.com/*","http://localhost:8080/*",
      		"http://123.206.231.254/spider/*"
     		],
      "js": ["jquery-1.10.2.js","income.js"],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://ir.baidu.com/*"],
      "js": ["jquery-1.10.2.js","income.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}